{
  "name": "wacz2car",
  "version": "1.0.0",
  "description": "Conver WACZ files to CAR files for uploading to IPFS.",
  "main": "index.js",
  "scripts": {
    "test": "ava",
    "build": "tsc",
    "lint": "ts-standard --fix ./src && tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webrecorder/wacz2car.git"
  },
  "keywords": [
    "warc",
    "wacz",
    "ipfs",
    "car",
    "upload",
    "p2p",
    "archive"
  ],
  "author": "WebRecorder",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/webrecorder/wacz2car/issues"
  },
  "homepage": "https://github.com/webrecorder/wacz2car#readme",
  "devDependencies": {
    "ava": "^5.2.0",
    "tape": "^5.6.3",
    "ts-node": "^10.9.1",
    "ts-standard": "^12.0.2",
    "typescript": "^5.0.0-beta"
  },
  "dependencies": {
    "@ipld/car": "^5.1.1",
    "@ipld/dag-pb": "^4.0.2",
    "@ipld/unixfs": "^2.1.1",
    "ipfs-car": "^1.0.0",
    "ipfs-unixfs": "^11.0.0",
    "multiformats": "^11.0.2"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
